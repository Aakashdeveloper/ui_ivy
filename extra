inline > styletag > stylesheet

h1
{
    color:red,
    font-size:30px,
    text-align:center,
    font-weight:bold
}

inline{
    color:red,
    font-size:30px
}

styletag{
    color:orange,
    font-size:40px,
    text-align:center
}

stylesheet{
    color:green,
    font-weight:bold,
    text-align:right
}

